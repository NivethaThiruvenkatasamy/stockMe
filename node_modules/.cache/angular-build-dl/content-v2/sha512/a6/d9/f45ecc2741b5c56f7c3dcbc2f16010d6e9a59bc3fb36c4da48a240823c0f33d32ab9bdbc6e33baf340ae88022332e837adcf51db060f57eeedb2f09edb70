        _angular_core__WEBPACK_IMPORTED_MODULE_2__["ÉµÉµproperty"]("innerHtml", ctx.content, _angular_core__WEBPACK_IMPORTED_MODULE_2__["ÉµÉµsanitizeHtml"]);
    } }, encapsulation: 2 });
