/***/ 59009:
/*!**************************************!*\
  !*** ./node_modules/qs/lib/index.js ***!
  \**************************************/
/***/ (function(module, __unused_webpack_exports, __webpack_require__) {

"use strict";


var stringify = __webpack_require__(/*! ./stringify */ 75672);
var parse = __webpack_require__(/*! ./parse */ 19913);
var formats = __webpack_require__(/*! ./formats */ 8157);

module.exports = {
    formats: formats,
    parse: parse,
    stringify: stringify
};


